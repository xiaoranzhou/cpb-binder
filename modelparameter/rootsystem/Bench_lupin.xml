<plant>
<seed name="Lupinus_Bench" subType="0">
    <parameter name="seedPos.x" value="0"/>
    <parameter name="seedPos.y" value="0"/>
    <parameter name="seedPos.z" value="-3"/>
    <parameter name="firstB" value="0" dev="0"/>
    <parameter name="delayB" value="0" dev="0"/>
    <parameter name="maxB" value="0" dev="0"/>
    <parameter name="maxTi" value="0" dev="0"/>
    <parameter name="nC" value="0"/>
    <parameter name="nz" value="0"/>
    <parameter name="firstSB" value="1000" dev="0"/>
    <parameter name="delaySB" value="1000" dev="0"/>
    <parameter name="delayRC" value="1000" dev="0"/>
    <parameter name="simulationTime" value="60" dev="0"/>
</seed>

<root name="taproot" subType="1">
    <parameter name="gf" value="1"/>
    <!--Growth function number [1]-->
    <parameter name="a" value="0.1223" dev="0.0149"/>
    <!--Root radius [cm]-->
    <parameter name="dx" value="0.2"/>
    <!--Axial resolution [cm] (maximal segment size)-->
    <parameter name="la" value="6.81" dev="1.9"/>
    <!--Apical zone [cm]-->
    <parameter name="lb" value="1.27" dev="1.19"/>
    <!--Basal zone [cm]-->
    <parameter name="lmax" value="150" dev="20"/>
    <!--Maximal root length [cm]-->
    <parameter name="ln" value="0.23" dev="0.13"/>
    <!--Inter-lateral distance [cm]-->
    <parameter name="r" value="2.91" dev="0.291"/>
    <!--Initial growth rate [cm day-1]-->
    <parameter name="rlt" value="1e+09"/>
    <!--Root life time [day]-->
    <parameter name="theta" value="0.2" dev="0.02"/>
    <!--Angle between root and parent root [rad]-->
	<parameter name="tropismT" value="1"/>
    <!--Type of root tropism (plagio = 0, gravi = 1, exo = 2, hydro, chemo = 3)-->
    <parameter name="tropismN" value="2"/>
    <!--Number of trials of root tropism-->
    <parameter name="tropismS" value="0.5"/>
    <!--Mean value of expected change of root tropism [1/cm]-->
    <parameter name="successor" type="2" percentage="1"/>
 </root>

